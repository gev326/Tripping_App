<h1> hi </h1>

<table>
<thead>
  <tr>
    <th> Date </th>
    <th> Event </th>
    <th> Time </th>
    <th> Location </th>
    <th> Information</th>
  </tr>
  </thead>


<tbody>
  <% @activity.each do |activity| %>
  <tr>
    <td><%= activity.date %></td>
    <td><%= activity.event %></td>
    <td><%= activity.time %></td>
    <td><%= activity.location %></td>
    <td><%= activity.information %></td>
    <td><%= link_to 'Edit', edit_activity_path(activity) %>
    <td><%= link_to 'Delete', activity, method: :delete, data: { confirm: 'Are you sure you want to cancel your trip?' } %> </td>

    </tr>
    <% end %>
    </tbody>
    </table>


</tbody>
